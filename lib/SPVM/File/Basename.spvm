class File::Basename {
  use File::Basename::Instance;
  
  # Class Methods
  static method fileparse : string[] ($path : string) {
    return File::Basename::Instance->new->fileparse($path);
  }
  
  static method basename : string ($path : string) {
    return File::Basename::Instance->new->basename($path);
  }
  
  static method dirname : string ($path : string) {
    return File::Basename::Instance->new->dirname($path);
  }
}
